ll,"original":{"line":3,"column":0},"generated":{"line":84,"column":1}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":84,"column":3}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":7}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":8}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":16}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":19}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":86,"column":27}}],"sources":{"src/views/UserForm.ts":"import { User } from '../models/User'\r\n\r\nexport class UserForm {\r\n    constructor(public parent: Element, public user: User) {\r\n        this.bindModel();\r\n    }\r\n\r\n    bindModel(): void {\r\n        this.user.on( 'change', () => {\r\n            this.render();\r\n        } );\r\n    }\r\n\r\n    eventsMap(): { [key: string]: () => void } {\r\n        return {\r\n            // \"click:button\": this.onButtonClick,\r\n            \"mouseenter:h1\": this.onHeaderHover,\r\n            \"click:.set-age\": this.onSetAgeClick\r\n        };\r\n    }\r\n\r\n    // //event implementation\r\n    // onButtonClick(): void {\r\n    //     console.log('Button clicked');\r\n    // }\r\n\r\n    //event implementation\r\n    onHeaderHover(): void {\r\n        console.log('H1 was hovered over');\r\n    }\r\n\r\n    //event implementation\r\n    onSetAgeClick = (): void => {\r\n        this.user.setRandomAge();\r\n        console.log(this.user.get(\"age\"));\r\n    }\r\n\r\n    template(): string {\r\n        return `\r\n            <div>\r\n                <h1>User Form</h1>\r\n                <div>User name: ${this.user.get('name')}</div>\r\n                <div>User age: ${this.user.get('age')}</div>\r\n                <input />\r\n                <button>Click Me</button>\r\n                <br/>\r\n                <button class=\"set-age\">Set Random Age</button>\r\n            </div>\r\n        `;\r\n    }\r\n\r\n    //helper method\r\n    bindEvents(fragment: DocumentFragment): void {\r\n        const eventsMap = this.eventsMap();\r\n\r\n        for (let eventKey in eventsMap) {\r\n            const [eventName, selector] = eventKey.split(':');\r\n\r\n            fragment.querySelectorAll(selector).forEach(element => {\r\n                //event handler\r\n                element.addEventListener(eventName, eventsMap[eventKey]);\r\n            });\r\n        }\r\n    }\r\n\r\n    render(): void {\r\n        this.parent.innerHTML = \"\";\r\n\r\n        const templateElement = document.createElement('template');\r\n        templateElement.innerHTML = this.template();\r\n\r\n        this.bindEvents(templateElement.content);\r\n\r\n        this.parent.append(templateElement.content);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"e9989a33d222a5ad5e155d6ce59971fa","cacheData":{"env":{}}}